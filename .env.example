# Aesyros Suite Environment Variables

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Application Environment
VITE_APP_ENV=development
VITE_APP_NAME=Aesyros Suite

# API Configuration
VITE_API_BASE_URL=https://api.aesyros.com
VITE_API_VERSION=v1

# Authentication
VITE_AUTH_REDIRECT_URL=http://localhost:5173
VITE_AUTH_GOOGLE_CLIENT_ID=your-google-client-id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REALTIME=true
VITE_ENABLE_AI_FEATURES=false

# Development Settings
VITE_DEV_TOOLS=true
VITE_LOG_LEVEL=debug

# External Services
VITE_SENTRY_DSN=your-sentry-dsn
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# File Upload
VITE_MAX_FILE_SIZE=10485760  # 10MB in bytes
VITE_ALLOWED_FILE_TYPES=pdf,doc,docx,txt,xlsx,csv

# App-specific settings
VITE_ALIGN_MAX_GOALS=100
VITE_PULSE_MAX_KPIS=50
VITE_FLOW_MAX_PROCESSES=200
VITE_CATALYST_MAX_JOURNEYS=25
VITE_FORESIGHT_MAX_SCENARIOS=10